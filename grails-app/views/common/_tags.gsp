<g:if test="${tags}">    <div class="w-clearfix associated-files-div-block">        <h3 class="eventstitle">Tags</h3>        <ul class="latest-news-list tags">            <blackFish:buildTagLink                    template="/common/tagLink"                    values="${tags}"                    class="tag"                    linkClass="tagLink"                    controller="tag"                    action="title"                    minSize="10"                    maxSize="10"/>        </ul>    </div></g:if>