<g:if test="${storyResult || fileResult}">	<div id="peopleList">		<g:if test="${storyResult}">		    <h3 class="modalTitle">Stories</h3>		    <hr />		    <g:render template="resultList" model="[objects: storyResult]" />		</g:if>		<g:if test="${fileResult}">		    <h3 class="modalTitle">Files</h3>		    <hr />		    <g:render template="resultList" model="[objects: fileResult]" />		</g:if>	</div></g:if><g:if test="${params.q && !fileResult && !storyResult}">    <p>There are no results for your query</p></g:if>