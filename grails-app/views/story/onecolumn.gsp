<g:set var="thumbnail" value="${grailsApplication.config.default_thumbnail}" /><html>	<head>		<g:render template="/story/head" />	</head>		<body>		<g:render template="/storyAdmin/adminInfo" />		<g:if test="${story?.primaryImage}">			<div class="primary-image-background">			    <div class="primary-image-overlay"></div>			    <div class="large-primary-image">		    		<g:render template="publicPrimaryImage" />		    	</div>		    	<div class="primary-image-text-container w-container wbg">		    		      		<h1 class="large-primary-image-heading" data-ix="on-load-move-left">${story.pageTitle}</h1>		      		<div class="large-primary-image-text" data-ix="on-load-move-left-2">${story.tagline}</div>		    	</div>			</div>		</g:if>		<div class="w-section">			<div class="body-text-container w-container wbgimage">				<g:if test="${!story?.primaryImage}">					<h1>						${story.pageTitle}					</h1>					<g:if test="${story.tagline}">						<h3>${story.tagline}</h3>					</g:if>				</g:if>				<div class="w-hidden-small w-hidden-tiny w-clearfix media-elements-block">					<g:if test="${story.pullQuote}">						<h2 class="pull-quote-text">${story.pullQuote}</h2>					</g:if>					<g:render template="/story/embedded" />					<g:render template="/story/embedded2" />				</div>				${story.description.encodeAsRaw()}				${story.bodyScript}				<div class="w-hidden-main w-hidden-medium w-clearfix media-elements-block">					<g:if test="${story.pullQuote}">						<h2 class="pull-quote-text">${story.pullQuote}</h2>					</g:if>					<g:render template="/story/embedded" />					<g:render template="/story/embedded2" />				</div>			</div>		</div>		<div class="w-container">			<div class="w-row">				<div class="w-col w-col-3">					<g:render template="/story/leftMenu" />				</div>				<div class="w-col w-col-3">					<g:render template="/story/relatedImages" />				</div>				<div class="w-col w-col-3">					<g:render template="/story/relatedContent" />				</div>				<div class="w-col w-col-3">					<g:render template="/common/tags" />				</div>			</div>		</div>		<!-- Related Stories -->				<g:render template="/story/relatedBottomStories" />		<g:render template="/storyAdmin/editButton" />	</body></html>