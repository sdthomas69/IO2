<g:if test="${story.embeddedImage2}">	<div class="w-clearfix embedded-image-block"><%--		<div class="photocred-text">--%><%--			${story.embeddedImage2?.caption ?: story.embeddedImage2.shortDescription}--%><%--		</div>--%>		<g:if test="${story.embeddedImage2.type == 'Image'}">		<a class="w-inline-block w-lightbox" href="#">			<blackFish:wflowChange 				sizes="100vw"				src="${resource(file: story.embeddedImage2.smallImage)}"				small="${resource(file: story.embeddedImage2?.smallImage)}"				medium="${resource(file: story.embeddedImage2?.mediumImage)}"				large="${resource(file: story.embeddedImage2?.mediumImage)}"				retina="${resource(file: story.embeddedImage2?.mediumImage)}"				width="100%"				title="${story.embeddedImage2.title}"				alt="${story.embeddedImage2.title}"			/>			<script class="w-json" type="application/json">              { "items": [{                "_id": "${story.embeddedImage2.id}",                "fileName": "${story.embeddedImage2.id}${story.embeddedImage2?.xLargeImage}",                "origFileName": "${story.embeddedImage2?.xLargeImage}",                "type": "image",                "url": "<g:resource file='${story.embeddedImage2?.xLargeImage}' />",                "caption": "${story.embeddedImage2?.caption ?: story.embeddedImage2?.shortDescription}"              }] }            </script>		</a>		</g:if>		<g:if test="${story.embeddedImage2.type == 'Quicktime'}">			<div id="container">Loading the player...</div>			<script type="text/javascript">				jwplayer("container").setup({					autostart: false,					width: "100%",				    aspectratio: "16:9",				    androidhls: true,			     	'image':'${story.embeddedImage2?.smallImage}',			     	'file':'${story.embeddedImage2?.path}'				});			</script>			<div class="clear">				<p>&nbsp;&nbsp;</p>			</div>		</g:if>		<g:elseif test="${story.embeddedImage2.type != 'Image' && story.embeddedImage2.type != 'Quicktime'}">			<g:link url="${story.embeddedImage2.path}">				<img class="embedded-image" src="<g:resource file='${story?.embeddedImage1?.smallImage}' />">			</g:link>		</g:elseif>		<div class="embedded-image-caption">			${story.embeddedImage2?.caption ?: story.embeddedImage2?.shortDescription}		</div>	</div></g:if>