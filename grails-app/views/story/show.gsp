<g:set var="thumbnail" value="${grailsApplication.config.default_thumbnail}"/><html><head>    <g:render template="/story/head"/></head><body><%--<g:if test="${story?.primaryImage}">    <div class="behind-main-navimage w-hidden-main w-hidden-medium"></div></g:if><g:if test="${!story?.primaryImage}">    <div class="behind-main-nav w-hidden-main w-hidden-medium"></div></g:if>--%><g:if test="${story?.primaryImage}">    <div class="primary-image-background">        <div class="primary-image-overlay"></div>        <div class="large-primary-image">            <g:render template="publicPrimaryImage"/>        </div>        <div class="primary-image-text-container w-container wbg">            <h1 class="large-primary-image-heading" data-ix="on-load-move-left">${story.pageTitle}</h1>            <div class="large-primary-image-text" data-ix="on-load-move-left-2">${story.tagline}</div>        </div>    </div></g:if><g:render template="/storyAdmin/adminInfo"/><g:if test="${story?.primaryImage}">    <div class="w-section">        <div class="body-text-container w-container wbgimage">            <div class="w-row">                <!-- Left Column -->                <div class="w-col w-col-9 w-clearfix description-text-column">                    <div class="small-primary-div-block">                    <%--							<g:if test="${story?.primaryImage}">--%>                    <%--								<div class="small-primary-image-text-block">--%>                    <%--									<h1 class="large-primary-image-heading small-primary-heading">--%>                    <%--										${story.pageTitle}--%>                    <%--									</h1>--%>                    <%--								</div>--%>                    <%--								<g:render template="publicPrimaryImage" />--%>                    <%--							</g:if>--%>                        <g:if test="${!story?.primaryImage}">                            <h1>                                ${story.pageTitle}                            </h1>                            <g:if test="${story.tagline}">                                <h3>${story.tagline}</h3>                            </g:if>                        </g:if>                    </div>                    <div class="w-hidden-small w-hidden-tiny w-clearfix media-elements-block">                        <g:if test="${story.pullQuote}">                            <h2 class="pull-quote-text">${story.pullQuote}</h2>                        </g:if>                        <g:render template="/story/embedded"/>                        <g:render template="/story/embedded2"/>                    </div>                    ${story.description.encodeAsRaw()}                    ${story.bodyScript}                    <div class="w-hidden-main w-hidden-medium w-clearfix media-elements-block">                        <g:if test="${story.pullQuote}">                            <h2 class="pull-quote-text">${story.pullQuote}</h2>                        </g:if>                        <g:render template="/story/embedded"/>                        <g:render template="/story/embedded2"/>                    </div>                </div>                <!-- Right Column -->                <div class="w-col w-col-3 sidebar-right">                    <!-- Menu -->                    <g:render template="/story/leftMenu"/>                    <!-- Images -->                    <g:render template="/story/relatedImages"/>                    <!-- Files -->                    <g:render template="/story/relatedContent"/>                    <!-- Tags -->                    <g:render template="/common/tags"/>                </div>            </div>        </div>    </div></g:if><g:if test="${!story?.primaryImage}">    <div class="w-section">        <div class="body-text-container primary-small w-container">            <div class="w-row">                <!-- Left Column -->                <div class="w-col w-col-9 w-clearfix description-text-column">                    <div class="small-primary-div-block">                    <%--							<g:if test="${story?.primaryImage}">--%>                    <%--								<div class="small-primary-image-text-block">--%>                    <%--									<h1 class="large-primary-image-heading small-primary-heading">--%>                    <%--										${story.pageTitle}--%>                    <%--									</h1>--%>                    <%--								</div>--%>                    <%--								<g:render template="publicPrimaryImage" />--%>                    <%--							</g:if>--%>                        <g:if test="${!story?.primaryImage}">                            <h1>                                ${story.pageTitle}                            </h1>                        </g:if>                    </div>                    <g:if test="${story.tagline}">                        <h3>${story.tagline}</h3>                    </g:if>                    <div class="w-hidden-small w-hidden-tiny w-clearfix media-elements-block">                        <g:if test="${story.pullQuote}">                            <h2 class="pull-quote-text">${story.pullQuote}</h2>                        </g:if>                        <g:render template="/story/embedded"/>                        <g:render template="/story/embedded2"/>                    </div>                    ${story.description.encodeAsRaw()}                    ${story.bodyScript}                    <div class="w-hidden-main w-hidden-medium w-clearfix media-elements-block">                        <g:if test="${story.pullQuote}">                            <h2 class="pull-quote-text">${story.pullQuote}</h2>                        </g:if>                        <g:render template="/story/embedded"/>                        <g:render template="/story/embedded2"/>                    </div>                </div>                <!-- Right Column -->                <div class="w-col w-col-3 sidebar-right">                    <!-- Menu -->                    <g:render template="/story/leftMenu"/>                    <!-- Images -->                    <g:render template="/story/relatedImages"/>                    <!-- Files -->                    <g:render template="/story/relatedContent"/>                    <!-- Tags -->                    <g:render template="/common/tags"/>                </div>            </div>        </div>    </div></g:if><!-- Related Stories --><g:render template="/story/relatedBottomStories"/><g:render template="/storyAdmin/editButton"/></body></html>