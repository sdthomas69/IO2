<g:if test="${story?.primaryImage}">    <g:set var ="link" value="${createLink(controller: 'file', action: 'title', params: ['title': story.primaryImage.urlTitle])}" />    <g:if test="${story.layout.toString() == 'ONECOLUMN' || story.layout.toString() == 'PORTAL'}">    	<g:set var="imageClass" value="large-primary-image" />    	<g:set var="slide" value="${true}" />    </g:if>    <g:if test="${story.primaryImage.type == 'Image'}">							<blackFish:wflowChange 			sizes="100vw"			src="${resource(file: slide ? story.primaryImage?.mediumSlideImage : story.primaryImage?.mediumImage)}"			small="${resource(file: slide ? story.primaryImage?.smallSlideImage : story.primaryImage?.smallImage)}"			medium="${resource(file: slide ? story.primaryImage?.slideImage : story.primaryImage?.mediumImage)}"			large="${resource(file: slide ? story.primaryImage?.mediumSlideImage : story.primaryImage?.largeImage)}"			retina="${resource(file: slide ? story.primaryImage?.xSlideImage : story.primaryImage?.xLargeImage)}"			width="100%"			class="${imageClass}"			title="${story.primaryImage.title}"			alt="${story.primaryImage.title}"		/>	</g:if>	<g:elseif test="${story.primaryImage.type == 'Quicktime'}">		<div id="container">Loading the player...</div>        <script type="text/javascript">			jwplayer("container").setup({				autostart: false,				width: "100%",				androidhls: true,			    aspectratio: "16:9",			    'image':'${story.primaryImage?.mediumImage ?: story.primaryImage.smallImage}',		     	<g:if test='${story.primaryImage?.stream}'>		     	sources: [					{'file': 'rtmp://stream.ocean.washington.edu/rsn/${story.primaryImage.flashURL}'}		     		<g:if test="${story.primaryImage.iPhoneURL}">		     			,{'file': '${story.primaryImage.iPhoneURL}'}		     		</g:if>				],			    rtmp: {bufferlength: 3}				</g:if>				<g:else>'file':'${story.primaryImage?.path}'</g:else>				});		</script>    </g:elseif>	<g:elseif test="${story.primaryImage.type == 'KML' || story.primaryImage.type == 'KMZ'}">		<div id="map3d" style="height: 600px; width: 690px;"></div>	</g:elseif>    <g:elseif test="${story.primaryImage.type == 'PDF'}">		<g:link url="${story.primaryImage.path}">			<img src="<g:resource file='${story.primaryImage.smallImage}' />" title="${story.primaryImage}" alt="${story.primaryImage}" width="150" border="0"/>		</g:link>	</g:elseif>	<g:else>		<img src="<g:resource file='${story.primaryImage.smallImage}' />" title="${story.primaryImage}" alt="${story.primaryImage}" width="150" border="0"/>	</g:else></g:if>