<g:if test="${story.embeddedImage1}">    <div class="w-clearfix embedded-image-block">    <%--		<div class="photocred-text">--%>    <%--			${story.embeddedImage1?.caption ?: story.embeddedImage1.shortDescription}--%>    <%--		</div>--%>        <g:if test="${story.embeddedImage1.type == 'Image'}">            <a class="w-inline-block w-lightbox" href="#">                <blackFish:wflowChange                        sizes="100vw"                        src="${resource(file: story.embeddedImage1.smallImage)}"                        small="${resource(file: story.embeddedImage1?.smallImage)}"                        medium="${resource(file: story.embeddedImage1?.mediumImage)}"                        large="${resource(file: story.embeddedImage1?.mediumImage)}"                        retina="${resource(file: story.embeddedImage1?.mediumImage)}"                        width="100%"                        title="${story.embeddedImage1.title}"                        alt="${story.embeddedImage1.title}"/>                <script class="w-json" type="application/json">              	{ "items": [{                "_id": "${story.embeddedImage1.id}",                "fileName": "${story.embeddedImage1.id}${story.embeddedImage1.xLargeImage}",                "origFileName": "${story.embeddedImage1.xLargeImage}",                "type": "image",                "url": "<g:resource file='${story.embeddedImage1.xLargeImage}'/>",                "caption": "${story?.embeddedImage1?.shortDescription}"              	}]}            	</script>            </a>        </g:if>        <g:if test="${story.embeddedImage1.type == 'Quicktime'}">            <div id="container">Loading the player...</div>            <script type="text/javascript">                jwplayer("container").setup({                    autostart: false,                    width: "100%",                    aspectratio: "16:9",                    androidhls: true,                    'image': '${story.embeddedImage1?.smallImage}',                    'file': '${story.embeddedImage1?.path}'                });            </script>            <div class="clear">                <p>&nbsp;&nbsp;</p>            </div>        </g:if>        <g:elseif test="${story.embeddedImage1.type != 'Image' && story.embeddedImage1.type != 'Quicktime'}">            <g:link url="${story.embeddedImage1.path}">                <img class="embedded-image" src="<g:resource file='${story?.embeddedImage1?.smallImage}'/>">            </g:link>        </g:elseif>        <div class="embedded-image-caption">            ${story.embeddedImage1?.caption ?: story.embeddedImage1.shortDescription}        </div>    </div></g:if>