<g:set var="thumbnail" value="${grailsApplication.config.default_thumbnail}" /><html>		<head>		<meta name="layout" content="main">		<meta property="og:title" content="Interactive Oceans" />		<meta property="og:url" content="${request.serverName}${request.forwardURI}" />		<meta property="og:description" content="${homeStory?.shortDescription}" />		<g:if test="${stories}">			<meta property="og:image" content="${stories[0]?.primaryImage?.mediumSlideImage}" />		</g:if>	</head>	<body>		<g:if test="${stories}">			<div class="slider w-slider" data-animation="slide" data-duration="500" data-infinite="1">				<div class="w-slider-mask">					<g:each in="${stories}" var="thisStory" status="i">						<div class="slide w-slide">							<div class="slide-overlay"></div>							<blackFish:wflowChange 								sizes="100vw"								src="${resource(file: thisStory?.primaryImage?.mediumSlideImage ?: thumbnail)}"								small="${resource(file: thisStory?.primaryImage?.smallSlideImage ?: thumbnail)}"								medium="${resource(file: thisStory?.primaryImage?.slideImage ?: thumbnail)}"								large="${resource(file: thisStory?.primaryImage?.mediumSlideImage ?: thumbnail)}"								retina="${resource(file: thisStory?.primaryImage?.xSlideImage ?: thumbnail)}"								width="100%"								title="${thisStory?.primaryImage?.title}"								alt="${thisStory?.primaryImage?.title}"							/>							<div class="hero-div-block w-container">								<a class="slideshowlinkbox w-inline-block" href="<blackFish:setLink object="${thisStory}" name="story" />">									<h1 class="hero-heading">${thisStory.pageTitle}</h1>									<div class="hero-text-block">										${thisStory?.tagline}									</div>								</a>							</div>						</div>					</g:each>				</div>				<g:if test="${stories.size() > 1 }">					<div class="w-clearfix w-slider-arrow-left">		      			<div class="arrowicon w-icon-slider-left"></div>		    		</div>		    		<div class="w-clearfix w-slider-arrow-right">		      			<div class="arrowicon w-icon-slider-right"></div>		    		</div>		    		<div class="navbutton w-round w-slider-nav"></div>	    		</g:if>			</div>		</g:if>				<g:if test="${homeStory}">			<div class="aboutsection" id="About">				<div class="about-container w-container">					<h1 class="mainheadingh1">${homeStory.tagline }</h1>					<div class="welcome-paragraph">${homeStory.description.encodeAsRaw()}</div>				</div>			</div>		</g:if>		<g:if test="${features}">			<div class="featured-stories-section">				<h1 class="mainheadingh1" id="Featured-Stories">Featured Topics</h1>				<div class="featuredstories-container w-container">					<div class="flex-wrap">						<g:each in="${features}" var="feature" status="i">							<a class="w-inline-block featured-story-link-box" href="<blackFish:setLink object="${feature}" name="story" />">								<blackFish:wflowChange 									sizes="(max-width: 767px) 96vw, (max-width: 991px) 222.671875px, 293.328125px"									src="${resource(file: feature?.primaryImage?.smallSlideImage ?: thumbnail)}"									small="${resource(file: feature?.primaryImage?.smallSlideImage ?: thumbnail)}"									medium="${resource(file: feature?.primaryImage?.slideImage ?: thumbnail)}"									large="${resource(file: feature?.primaryImage?.mediumSlideImage ?: thumbnail)}"									width="100%"									title="${feature?.primaryImage?.title}"									alt="${feature?.primaryImage?.title}"								/>								<div class="related-topics-div-block">									<h4>${feature.pageTitle}</h4>									<p>${feature.shortDescription}</p>								</div>							</a>						</g:each>					</div>				</div>			</div>		</g:if>	</body></html>