<html><head>	<title>CES Map</title>	<meta name="layout" content="main" />	<link rel="stylesheet" href="https://unpkg.com/leaflet@1.0.2/dist/leaflet.css" />	<script src="https://unpkg.com/leaflet@1.0.2/dist/leaflet.js"></script>	<script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>	<%--		<link rel="stylesheet" type="text/css" href="<g:resource dir='css' file='map.css' />" />--%>	<link rel="stylesheet" type="text/css" href="<g:resource dir='cluster' file='screen.css' />" />	<link rel="stylesheet" type="text/css" href="<g:resource dir='cluster' file='MarkerCluster.css' />" />	<link rel="stylesheet" type="text/css" href="<g:resource dir='cluster' file='MarkerCluster.Default.css' />" />	<script type="text/javascript" src="<g:resource dir='cluster' file='leaflet.markercluster-src.js' />"></script></head><body><div id='map'></div><script>    //var map = L.map('map');    var map = L.map('map', {        center: [0, 0],        zoom: 1,        crs: L.CRS.EPSG4326    });    var buoyIcon = L.icon({        iconUrl: '<g:resource file="images/instrument3-icon.png" />', iconSize: [28,28]    });    var markers = L.markerClusterGroup();    var wmsLayer = L.tileLayer.wms('https://maps.oceanobservatories.org/mapserv?map=/public/mgg/web/gmrt.marine-geo.org/htdocs/services/map/wms_merc.map&', {layers: 'topo',        format: 'image/png',        transparent: true,        crs: L.CRS.EPSG4326,        attribution: 'Global Multi-Resolution Topography (GMRT), Version 3.2'    }).addTo(map);    var geoJsonData = {};    $.ajax({        //url: "http://localhost:5000/data",        url: "/files/geoJson.json",        async: false,        dataType: 'json',        success: function(data) {            geoJsonData = data;        }    });    var geoJsonLayer = L.geoJson(geoJsonData, {        onEachFeature: function (feature, layer) {            layer.bindPopup(feature.properties.display_name);        },        pointToLayer: function(feature, latlng) {            var marker = L.marker(latlng, {icon: buoyIcon});            marker.bindPopup(feature.properties.display_name);            return marker;        }    });    markers.addLayer(geoJsonLayer);    map.addLayer(markers);    map.fitBounds(markers.getBounds());</script></body></html>