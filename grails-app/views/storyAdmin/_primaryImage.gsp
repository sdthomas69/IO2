<g:link controller='file' action='show' id='${story.primaryImage.id}'>	<g:if test="${story.primaryImage.type == 'Image'}">		<img src="<g:resource file='${story.primaryImage.slideImage}' />" alt="${story.primaryImage}"/>	</g:if>	<g:else>		<img src="<g:resource file='${story.primaryImage.tinyImage}' />" alt="" />	</g:else></g:link><ul class="w-list-unstyled w-clearfix admin-nav-bar-list file-edit-list">	<li class="admin-nav-bar-list-item">		<g:link class="w-inline-block admin-link red" 	    	title="Remove"			alt="Remove"	    	controller='storyAdmin' 	    	params='["id":story.id, "fileId":story.primaryImage.id]' 	    	action='unSetPrimaryImage'>	    	<div class="admin-link-text-block file-button">Remove</div>	    </g:link>    </li>   	<li class="admin-nav-bar-list-item">	    <g:link class="w-inline-block admin-link"	    	title="Set as EmbeddedImage 1"			alt="Set as EmbeddedImage 1"	    	controller='storyAdmin' 	    	params='["id":story.id, "fileId":story.primaryImage.id]' action='embedImage1'>	    	<div class="admin-link-text-block file-button">E1</div>	    </g:link>    </li>    <li class="admin-nav-bar-list-item">		<g:link class="w-inline-block admin-link"			title="Set as EmbeddedImage 2"			alt="Set as EmbeddedImage 2"			controller='storyAdmin' 			params='["id":story.id, "fileId":story.primaryImage.id]' 			action='embedImage2'>			<div class="admin-link-text-block file-button">E2</div>		</g:link>	</li>	<li class="admin-nav-bar-list-item">		<g:link class="w-inline-block admin-link"			title="Associate"			alt="Associate"			controller='storyAdmin' 			params='["id":story.id, "files":story.primaryImage.id]' 			action='addFiles'>			<div class="admin-link-text-block file-button">Associate</div>		</g:link>	</li></ul>