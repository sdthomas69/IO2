<html><head>    <meta name="layout" content="main"/>    <script type="text/javascript" src="<g:resource dir='jwplayer' file='jwplayer.js'/>"></script>    <script>jwplayer.key = "${grailsApplication.config.jwID}";</script>    <title>${file}</title></head><body><div id="adminBg" class="row">    <div class="row">        <div class="large-12 columns">            <g:render template="/fileAdmin/adminInfo"/>        </div>    </div>    <div class="row">        <div class="large-12 columns">            <h2>${file}</h2>            <div id="container">Loading the player...</div>            <script type="text/javascript">                jwplayer("container").setup({                    autostart: false,                    width: "100%",                    aspectratio: "16:9",                    androidhls: true,                    'image': '${file?.mediumImage ?: file.smallImage}',                    sources: [                        {'file': 'rtmp://stream.ocean.washington.edu/rsn/${file.flashURL}'}                        <g:if test="${file.iPhoneURL}">                        , {'file': '${file.iPhoneURL}'}                        </g:if>                    ],                    rtmp: {bufferlength: 3}                });            </script>        </div>        <div class="large-12 columns">            ${file.description}            <g:if test="${file.caption}">                <br/>${file.caption}            </g:if>            <ul>                <li>File Size: ${file.measuredSize}</li>                <li>Date: <g:formatDate format="yyyy-MM-dd" date="${file.date}"/></li>            </ul>            <g:render template="/file/relatedStories"/>            <g:render template="/common/tags"/>            <g:render template="/fileAdmin/editButton"/>        </div>    </div></div></body></html>