<g:set var="thumbnail" value="${grailsApplication.config.default_thumbnail}" /><html>	<head>		<meta name="layout" content="main">		<meta property="og:title" content="${file}" />		<meta property="og:url" content="${request.serverName}${request.forwardURI}" />		<meta property="og:description" content="${file.shortDescription}" />		<meta property="og:image" content="${file.mediumSlideImage ?: file.smallImage}" />		<title>${file}</title>		<g:if test="${file?.type == 'Quicktime'}">			<script type="text/javascript" src="<g:resource dir='jwplayer' file='jwplayer.js' />"></script>		</g:if>	</head>		<body>		<g:render template="/fileAdmin/adminInfo" />		<div class="w-section">			<div class="w-container body-text-container primary-small">				<div class="w-row">										<!-- Left Column -->										<div class="w-col w-col-9 w-clearfix description-text-column">						<g:if test="${file.type == 'Quicktime'}">							<div id="container">Loading the player...</div>							<script type="text/javascript">								jwplayer("container").setup({									autostart: false,									width: "100%",								    aspectratio: "16:9",								    androidhls: true,							     	'image':'${file?.mediumImage ?: file.smallImage}',							     	'file':'${file?.path}'								});							</script>							<div class="clear">								<p>&nbsp;&nbsp;</p>							</div>						</g:if>						<g:elseif test="${file.type == 'Image'}">							<div class="small-primary-div-block">								<h2>${file}</h2>								<g:link url="${file.path}">									<blackFish:wflowChange 										sizes="100vw"										src="${resource(file: file.mediumImage)}"										small="${resource(file: file?.smallImage)}"										medium="${resource(file: file?.mediumImage)}"										large="${resource(file: file?.largeImage)}"										retina="${resource(file: file?.xLargeImage)}"										width="100%"										title="${file.title}"										alt="${file.title}"									/>								</g:link>							</div>						</g:elseif>						<g:elseif test="${file.type != 'Image' && file.type != 'Quicktime'}">							<h2>${file}</h2>							<g:link url="${file.path}">								<img src="<g:resource file='${file?.smallImageSquared ?: thumbnail}' />" width="60"/>							</g:link>						</g:elseif>					</div>										<!-- Right Column -->										<div class="w-col w-col-3 sidebar-right">						${file.description}						<ul>							<g:if test="${file.type}">								<li>Type: ${file.type}</li>							</g:if>							<g:if test="${file.measuredSize}">								<li>Size: ${file.measuredSize}</li>							</g:if>						</ul>						<g:render template="/file/relatedStories" />						<g:render template="/common/tags" />					</div>				</div>				<g:render template="/fileAdmin/editButton" />			</div>		</div>			</body></html>